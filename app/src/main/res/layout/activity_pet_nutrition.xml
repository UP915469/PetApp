<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PetNutrition">

    <ImageView
        android:id="@+id/food_img_3"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:layout_marginTop="168dp"
        android:contentDescription="@string/pet_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <ImageView
        android:id="@+id/food_img_2"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:layout_marginTop="168dp"
        android:contentDescription="@string/pet_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/food_img_1"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:layout_marginTop="168dp"
        android:contentDescription="@string/pet_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pet_name2"
        android:layout_width="107dp"
        android:layout_height="38dp"
        android:layout_marginTop="52dp"
        android:text="@string/pet_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.717"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.478" />

    <ImageView
        android:id="@+id/pet_img2"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:layout_marginTop="40dp"
        android:contentDescription="@string/pet_img"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/food_name_1"
        android:layout_width="90dp"
        android:layout_height="23dp"
        android:layout_marginStart="52dp"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="48dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/food_img_1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/food_name_2"
        android:layout_width="92dp"
        android:layout_height="22dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="49dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/food_name_3"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toEndOf="@+id/food_name_1"
        app:layout_constraintTop_toBottomOf="@+id/food_img_2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/food_name_3"
        android:layout_width="93dp"
        android:layout_height="22dp"
        android:layout_marginEnd="52dp"
        android:layout_marginRight="52dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/food_img_3"
        app:layout_constraintVertical_bias="0.109" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="234dp"
        android:layout_height="46dp"
        android:layout_marginBottom="32dp"
        android:gravity="center"
        android:text="Click a food item to see stores that sell it near you!"
        app:layout_constraintBottom_toTopOf="@+id/nutrition_comments"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/nutrition_comments"
        android:layout_width="299dp"
        android:layout_height="207dp"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginBottom="104dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>